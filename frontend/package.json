{
  "name": "smart-stock-insider-frontend",
  "version": "1.0.0",
  "description": "智股通前端应用 - 基于Tauri 2.0的桌面投资研究平台",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:unit": "vitest run src/test/**/*.test.{ts,tsx}",
    "test:integration": "vitest run src/test/integration/**/*.test.{ts,tsx}",
    "test:e2e": "playwright test",
    "test:component": "vitest run src/test/components/**/*.test.{ts,tsx}",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=verbose",
    "test:clean": "rimraf coverage test-results node_modules/.cache",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-shell": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "antd": "^5.12.8",
    "@ant-design/icons": "^5.2.6",
    "@ant-design/colors": "^7.0.2",
    "zustand": "^4.4.7",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "echarts-for-react": "^3.0.2",
    "recharts": "^2.8.0",
    "lightweight-charts": "^4.1.3",
    "react-beautiful-dnd": "^13.1.1",
    "react-grid-layout": "^1.4.4",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8",
    "react-intersection-observer": "^9.5.3",
    "framer-motion": "^10.16.16",
    "react-hotkeys-hook": "^4.4.4",
    "react-use": "^17.4.2",
    "lodash-es": "^4.17.21",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-virtualized": "^9.21.29",
    "@types/react-window": "^1.8.8",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/blocks": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/react-vite": "^7.6.5",
    "@storybook/testing-library": "^0.2.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.6.5",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "cssnano": "^6.0.2",
    "sass": "^1.69.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}